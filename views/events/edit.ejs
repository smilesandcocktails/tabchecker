<div class="panel panel-default">
  <div class="panel-body">
    <div class="col-md-12">
    <h2>Edit <%-foundEvent.eventName%></h2>
  </div>
  <div class="col-md-12">
    <a href="/events" class="btn btn-warning">Back to All Events</a>
    <form action="/events/add" method="POST">
      <div class="form-group">
          <label for="eventName">Event Name:</label>
          <input type="text" name="eventName" value="<%=foundEvent.eventName%>" class="form-control">
        </div>
        <div class="form-group">
          <label for="amountOwe">Event Date:</label>
          <input type="date" name="date" value="<%=foundEvent.date%>" class="form-control">
        </div>
        <div class="form-group">
          <label for="amountOwe">Total Bill:</label>
          <input type="number" name="totalBill" value="<%=foundEvent.totalBill%>" class="form-control">
        </div>
        <div class="form-group">
          <label for="amountOwe">Payer:</label>
          <input type="text" name="payer" value="<%=foundEvent.payer%>" class="form-control">
        </div>
        <div class="form-group">
          <button type="submit" class="btn btn-primary">Update</button>
        </div>
    </form>
  </div>
  </div>
</div>